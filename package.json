{
    "name": "moh-sitereg",
    "version": "2.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --host 0.0.0.0 --port 4200 -o --disable-host-check",
        "dev": "ng serve --host 0.0.0.0 -o",
        "prebuild": "node src/version.js",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "e2e-protractor": "npx protractor",
        "e2e:registration": "npm run e2e-protractor -- e2e/protractor.registration.conf.js",
        "e2e-register:happy-path": "npm run e2e-protractor -- e2e/protractor.registration.conf.js --specs=e2e/registration/src/reg-happy-path-e2e-spec.ts",
        "e2e-register:happy-path-dev": "npm run e2e-protractor -- e2e/protractor.registration.conf.js --specs=e2e/registration/src/reg-happy-path-e2e-spec.ts --data=e2e/data.json --baseUrl=https://moh-sitereg-dev.pathfinder.gov.bc.ca",
        "e2e-register:happy-path-local": "npm run e2e-protractor -- e2e/protractor.registration.conf.js --specs=e2e/registration/src/reg-happy-path-e2e-spec.ts --data=e2e/data.json",
        "e2e-update:happy-path": "npm run e2e-protractor -- e2e/protractor.update.conf.js --specs=e2e/update/src/update-happy-path.e2e-spec.ts",
        "e2e-update:happy-path-dev": "npm run e2e-protractor -- e2e/protractor.update.conf.js --specs=e2e/update/src/update-happy-path.e2e-spec.ts --data=e2e/data-update.json --baseUrl=https://moh-sitereg-dev.pathfinder.gov.bc.ca",
        "e2e-update:happy-path-local": "npm run e2e-protractor -- e2e/protractor.update.conf.js --specs=e2e/update/src/update-happy-path.e2e-spec.ts --data=e2e/data-update.json ",
        "e2e:update": "npm run e2e-protractor -- e2e/protractor.update.conf.js",
        "format": "prettier --write \"src/**/*.ts\" && npm run lint && npm run build",
        "publish": "ng build --prod --output-hashing none",
        "webdriver-update": "webdriver-manager update"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~8.2.9",
        "@angular/common": "~8.2.9",
        "@angular/core": "~8.2.9",
        "@angular/forms": "~8.2.9",
        "@angular/platform-browser": "~8.2.9",
        "@angular/platform-browser-dynamic": "~8.2.9",
        "@angular/router": "~8.2.9",
        "@ng-select/ng-select": "^2.20.5",
        "@types/faker": "^4.1.5",
        "angular2-text-mask": "^9.0.0",
        "angular2-uuid": "^1.1.1",
        "blueimp-load-image": "^2.20.1",
        "bootstrap": "^4.0.0",
        "classlist.js": "^1.1.20150312",
        "core-js": "^2.5.4",
        "faker": "^4.1.0",
        "font-awesome": "^4.7.0",
        "jquery": "^3.4.1",
        "moh-common-lib": "^2.1.3",
        "moment": "^2.24.0",
        "mygovbc-bootstrap-theme": "^0.4.1",
        "ngx-bootstrap": "^3.2.0",
        "ngx-json-viewer": "^2.4.0",
        "ngx-mask": "^7.3.6",
        "ngx-mydatepicker": "^2.4.5",
        "pdfjs-dist": "^2.0.550",
        "popper.js": "^1.14.7",
        "prettier": "^1.16.4",
        "rxjs": "^6.3.3",
        "screenshot-protractor": "^1.0.0",
        "sha1": "^1.1.1",
        "tslib": "^1.10.0",
        "yargs": "^13.2.4",
        "zlib": "^1.0.5",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.803.23",
        "@angular/cli": "~8.3.20",
        "@angular/compiler": "^8.2.14",
        "@angular/compiler-cli": "^8.2.9",
        "@angular/language-service": "~8.2.9",
        "@types/jasmine": "~3.5.10",
        "@types/jasminewd2": "~2.0.8",
        "@types/node": "~8.9.4",
        "codelyzer": "^4.5.0",
        "jasmine": "^3.5.0",
        "jasmine-core": "~3.5.0",
        "jasmine-json-test-reporter": "^1.0.0-beta",
        "jasmine-reporters": "^2.3.2",
        "jasmine-spec-reporter": "~5.0.1",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~2.1.1",
        "karma-jasmine": "~3.1.1",
        "karma-jasmine-html-reporter": "^1.5.3",
        "protractor": "~5.4.3",
        "protractor-screenshot-utils": "^1.0.5",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.4.5"
    }
}
